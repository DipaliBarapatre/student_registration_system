<h1>Students List</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Date of Birth</th>
    <th>Address</th>
    <th>Photo</th>
    <th>Actions</th>
  </tr>
  <% @students.each do |student| %>
    <tr>
      <td><%= student.name %></td>
      <td><%= student.email %></td>
      <td><%= student.date_of_birth %></td>
      <td><%= student.address %></td>
      <td><%= link_to 'Edit', edit_student_path(student) %></td>
      <td><%= link_to 'Show', student_path(student) %></td>
      <td><%= link_to 'Verify', student_path(student) %></td>
    </tr>
  <% end %>
</table>
